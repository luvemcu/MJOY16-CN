 235:usbdrv/usbdrvasm.S ****     ldi     resCrcH, 0
 236:usbdrv/usbdrvasm.S ****     ldi     polyL, lo8(0xa001)
 237:usbdrv/usbdrvasm.S ****     ldi     polyH, hi8(0xa001)
