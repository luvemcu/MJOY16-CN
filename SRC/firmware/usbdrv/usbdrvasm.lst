 241:usbdrv/usbdrvasm.S **** usbCrcByteLoop:
 242               	    ld      byte, ptr+
 243:usbdrv/usbdrvasm.S ****     eor     resCrcL, byte
 244:usbdrv/usbdrvasm.S **** usbCrcBitLoop:
 245               	    ror     resCrcH     ; carry is always set here (see brcs jumps to here)
